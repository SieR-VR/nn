{
  "scopeName": "source.nn",
  "patterns": [
    {
      "name": "entity.name.function",
      "match": "\\b(?!Tensor)[a-zA-Z_][a-zA-Z0-9_]*\\b(?=\\s*[\\(\\[])"
    },
    {
      "name": "support.type",
      "match": "Tensor"
    },
    {
      "name": "keyword.operator",
      "match": "\\|>|=|\\+|\\-|\\*|\\/"
    },
    {
      "name": "variable.parameter",
      "match": "\\b(?!Tensor)[a-zA-Z_][a-zA-Z0-9_]*\\b(?![\\[\\(])"
    },
    {
      "name": "constant.numeric",
      "match": "[1-9][0-9]*"
    },
    {
      "name": "string.quoted.double",
      "begin": "\"",
      "end": "\""
    },
    {
      "name": "string.quoted.single",
      "begin": "'",
      "end": "'"
    }
  ]
}
