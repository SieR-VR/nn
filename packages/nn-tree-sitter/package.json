{
  "name": "nn-tree-sitter",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "node-gyp-build": "^4.8.1",
    "tree-sitter": "^0.21.1",
    "tree-sitter-cli": "^0.23.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "prebuildify": "^6.0.1"
  },
  "scripts": {
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "parse": "tree-sitter parse",
    "test": "node --test bindings/node/*_test.js",
    "clean": "rm -rf bindings/ build/ src/",
    "hash-compare": "node scripts/hash-compare.js",
    "build": "yarn hash-compare && yarn build:full",
    "build:full": "yarn clean && tree-sitter generate && node-gyp-build"
  },
  "main": "bindings/node/index.js",
  "types": "bindings/node",
  "typings": "bindings/node/index.d.ts",
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "tree-sitter": [
    {
      "scope": "source.nn",
      "injection-regex": "^nn$"
    }
  ]
}
